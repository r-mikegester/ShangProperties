{
    "headers": [
        {
            "source": "/fonts/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=604800, immutable"
                }
            ]
        },
        {
            "source": "/(.*)\\.(webp|jpg|jpeg|png|gif)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}